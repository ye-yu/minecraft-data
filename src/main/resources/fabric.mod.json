{
  "schemaVersion": 1,
  "id": "$modId",
  "version": "$version",
  "name": "$name",
  "description": "$description",
  "authors": [
    "YeYu"
  ],
  "contact": {
    "homepage": "https://github.com/ye-yu",
    "sources": "https://github.com/ye-yu"
  },
  "license": "GPLv3",
  "icon": "assets/$modId/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "fp.yeyu.mcdata.PlayData"
      }
    ]
  },
  "mixins": [
    "pd.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "fabric-language-kotlin": ">=$kotlinVersion",
    "minecraft": "$minecraftVersion"
  },
  "accessWidener": "playdata.accesswidener",
  "suggests": {
    "flamingo": "*"
  }
}
